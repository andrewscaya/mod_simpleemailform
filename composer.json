{
    "name": "andrewscaya/mod_simpleemailform",
    "description": "Joomla module: mod_simpleemailform",
    "type": "project",
    "license": "GPL-2.0",
    "keywords": [
        "joomla",
        "simple email form",
        "module",
        "extension"
    ],
    "homepage": "http://joomla.unlikelysource.org/",
    "authors": [
        {
            "name": "Doug Bierer",
            "email": "doug@unlikelysource.com",
            "homepage": "http://unlikelysource.net/"
        },
        {
            "name": "Andrew Caya",
            "email": "andrewscaya@yahoo.ca",
            "homepage": "https://github.com/andrewscaya",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.10",
        "joomla/application": "~1.9",
        "joomla/archive": "^1.1.7",
        "joomla/data": "~1.2",
        "joomla/di": "~1.5",
        "joomla/event": "~1.2",
        "joomla/filter": "^1.4.3",
        "joomla/image": "^1.5",
        "joomla/input": "~1.3",
        "joomla/ldap": "~1.2",
        "joomla/registry": "^1.6",
        "joomla/session": "^1.3.4",
        "joomla/string": "^1.4",
        "joomla/uri": "~1.2",
        "joomla/utilities": "^1.6",
        "ircmaxell/password-compat": "1.*",
        "leafo/lessphp": "dev-master",
        "paragonie/random_compat": "~1.4",
        "paragonie/sodium_compat": "1.9.1",
        "phpmailer/phpmailer": "^5.2.20",
        "symfony/polyfill-php55": "~1.9",
        "symfony/polyfill-php56": "~1.9",
        "symfony/polyfill-php71": "~1.9",
        "symfony/polyfill-php73": "~1.9",
        "symfony/yaml": "2.*",
        "simplepie/simplepie": "1.3.1",
        "google/recaptcha": "^1.1",
        "typo3/phar-stream-wrapper": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35",
        "mockery/mockery": "^0.9.7",
        "psr/log": "^1.0.0",
        "squizlabs/php_codesniffer": "^2.3",
        "phpdocumentor/phpdocumentor": "2.9.0",
        "pds/skeleton": "~1.0"
    },
    "autoload": {
        "psr-0": {
            "sefv2": "./sefv2"
        },
        "classmap": [
            "./",
            "./tests/unit"
        ],
        "exclude-from-classmap": [
            "./vendor/joomla-projects",
            "./vendor/consolidation"
        ]
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "cs": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit",
        "test-cover": "phpunit --coverage-clover build/logs/clover.xml",
        "test-report": "test-reporter --coverage-report build/logs/clover.xml",
        "apidocs": "phpdoc --template=responsive-twig"
    }
}
